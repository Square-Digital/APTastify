{
  "family": "aptastify-ui-staging",
  "containerDefinitions": [
      {
          "name": "aptastify-ui-staging",
          "image": "325066841340.dkr.ecr.us-east-2.amazonaws.com/aptastify-fe",
          "cpu": 256,
          "memory": 512,
          "portMappings": [
              {
                  "name": "aptastify-ui-staging-80-tcp",
                  "containerPort": 80,
                  "hostPort": 80,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "environment": [],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "ulimits": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/aptastify-ui-staging",
                  "awslogs-create-group": "true",
                  "awslogs-region": "us-east-2",
                  "awslogs-stream-prefix": "ecs"
              },
              "secretOptions": []
          },
          "systemControls": []
      }
  ],
  "executionRoleArn": "arn:aws:iam::325066841340:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false
}
